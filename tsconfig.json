/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "sourceMap": true,
    "paths": {
      "ngym-attachment": [
        "dist/ngym-attachment/ngym-attachment",
        "dist/ngym-attachment"
      ],
      "ngym-chips": [
        "dist/ngym-chips/ngym-chips",
        "dist/ngym-chips"
      ],
      "ngym-home": [
        "dist/ngym-home/ngym-home",
        "dist/ngym-home"
      ],
      "ngym-input": [
        "dist/ngym-input/ngym-input",
        "dist/ngym-input"
      ]
    },
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2017",
    "module": "es2020",
    "lib": [
      "es2020",
      "dom"
    ]
  },
  "strictPropertyInitialization": false
}